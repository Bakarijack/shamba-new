<div class="container-fluid form-section">
    <h4 class="register-header">Register Land</h4>
    <div class="row mt-5">
        <div class="col-lg-8 col-md-12 col-sm-12">
            <div class="sub-header">
                <h6 class="section-header">Section : <i class="fa fa-a latter"></i></h6>
                <p>Title Deed Details</p>
            </div>
            <form>
                <div class="container">
                    <div class="row">

                        <div class="col-lg-12 col-md-12 col-sm-12 mt-1">
                            <div class="container">
                                <div class="row">

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Owner name</label>
                                            <input type="text" id="name" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">ID</label>
                                            <input type="text" id="id_num" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12 mt-1">
                            <div class="container">
                                <div class="row">

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Land number</label>
                                            <input type="text" id="land_num" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Title number</label>
                                            <input type="text" id="title_num" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12 mt-1">
                            <div class="container">
                                <div class="row">

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Seal</label>
                                            <input type="text" id="seal" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Date of issue</label>
                                            <input type="date" id="date_of_issue" class="form-control" />
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12 mt-1">
                            <div class="container">
                                <div class="row">

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Map sheet number</label>
                                            <input type="text" id="map_num" class="form-control" />
                                        </div>
                                    </div>

                                    <div class="col-lg-6 col-md-12 col-sm-12">
                                        <div class="form-outline">
                                            <label class="form-label" for="form6Example1">Estimated size of land</label>
                                            <input type="text" id="land_size" class="form-control" />
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div>
                        <button type="submit"
                            class="btn btn-primary btn-block mb-4 col-lg-12 col-md-12 col-sm-12 mt-1">Place
                            order</button>
                    </div>
                </div>
            </form>
        </div>
        <div class="col-lg-4 col-md-12 col-sm-12"></div>
    </div>
</div>